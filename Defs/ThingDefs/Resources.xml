<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <ThingDef ParentName="ResourceBase">
    <defName>Graphite</defName>
    <label>graphite</label>
    <description>Naturally-occurring form of crystalline carbon. Has a wide range of uses in metallurgy and manufacturing.</description>
    <graphicData>
      <texPath>Things/Item/Resource/Graphite</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Metal_Drop</soundDrop>
    <useHitPoints>true</useHitPoints>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MarketValue>11</MarketValue>
      <Mass>0.22</Mass>
      <StuffPower_Armor_Sharp>0.75</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>1.25</StuffPower_Armor_Blunt>
      <StuffPower_Armor_Heat>1.5</StuffPower_Armor_Heat>
      <StuffPower_Insulation_Cold>4</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>7</StuffPower_Insulation_Heat>
      <SharpDamageMultiplier>0.75</SharpDamageMultiplier>
      <BluntDamageMultiplier>0.5</BluntDamageMultiplier>
	  <Flammability>0.1</Flammability>
    </statBases>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
    <deepCommonality>1</deepCommonality>
    <deepCountPerPortion>15</deepCountPerPortion>
    <deepLumpSizeRange>5~13</deepLumpSizeRange>
    <burnableByRecipe>false</burnableByRecipe>
    <smeltable>true</smeltable>
    <stuffProps>
      <categories>
        <li>Metallic</li>
      </categories>
      <commonality>0.02</commonality>
      <constructEffect>ConstructMetal</constructEffect>
      <color>(46,47,48)</color>
      <soundImpactStuff>BulletImpact_Metal</soundImpactStuff>
      <soundMeleeHitSharp>MeleeHit_Metal_Sharp</soundMeleeHitSharp>
      <soundMeleeHitBlunt>MeleeHit_Metal_Blunt</soundMeleeHitBlunt>
      <statFactors>
        <Beauty>0.8</Beauty>
        <DoorOpenSpeed>1</DoorOpenSpeed>
        <MaxHitPoints>2.2</MaxHitPoints>
        <WorkToBuild>1</WorkToBuild>
        <MeleeWeapon_CooldownMultiplier>0.7</MeleeWeapon_CooldownMultiplier>
        <Flammability>0.1</Flammability>
      </statFactors>
    </stuffProps>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
  </ThingDef>
  
    <ThingDef ParentName="ResourceBase">
    <defName>ComponentUltra</defName>
    <label>nano component</label>
    <description>An ultra-tech components, utilized in most complex technological marvels know to humanity..</description>
    <graphicData>
      <texPath>Things/Item/Carbon_NanoComponent</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <drawSize>0.7</drawSize>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>50</stackLimit>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>70</MaxHitPoints>
      <MarketValue>500</MarketValue>
      <Mass>0.6</Mass>
      <Flammability>0.1</Flammability>
      <DeteriorationRate>2.0</DeteriorationRate>
    </statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
    <thingSetMakerTags><li>RewardStandardHighFreq</li></thingSetMakerTags>
  </ThingDef>
  
    <RecipeDef>
    <defName>Make_ComponentUltra</defName>
    <label>Make nano component</label>
    <description>Make nano component.</description>
    <jobString>Making nano component.</jobString>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>15000</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	<unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>CarbonPlates</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>ComponentSpacer</li>
          </thingDefs>
        </filter>
        <count>150</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>2</count>
     </li>
	 <li>
        <filter>
          <thingDefs>
            <li>Gold</li>
          </thingDefs>
        </filter>
        <count>0.5</count>
     </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>CarbonPlates</li>
		<li>ComponentSpacer</li>
		<li>ComponentIndustrial</li>
		<li>Gold</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <ComponentUltra>1</ComponentUltra>
    </products>
	 <recipeUsers>
      <li>FabricationBench</li>
    </recipeUsers>
	 <skillRequirements>
      <Crafting>10</Crafting>
    </skillRequirements>
	 <researchPrerequisite>UltraFabrication</researchPrerequisite>
  </RecipeDef>
  
